{
  "expo": {
    "name": "callcenter",
    "slug": "callcenter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyC4d9HJz5nBz1ZXochyb-J3vwLopdw_9H8"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show you nearby places."
      },
      "bundleIdentifier": "com.anonymous.callcenter"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC4d9HJz5nBz1ZXochyb-J3vwLopdw_9H8"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.callcenter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
